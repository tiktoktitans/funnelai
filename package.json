{
  "name": "funnelai",
  "version": "1.0.0",
  "private": true,
  "description": "AI-powered funnel builder and deployment platform",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "typecheck": "turbo typecheck",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,json}\"",
    "clean": "turbo clean && rm -rf node_modules .turbo",
    "db:generate": "pnpm --filter @funnelai/database generate",
    "db:push": "pnpm --filter @funnelai/database push",
    "db:seed": "pnpm --filter @funnelai/database seed"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.17",
    "@anthropic-ai/sdk": "^0.16.1",
    "@clerk/nextjs": "^4.29.3",
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@supabase/supabase-js": "^2.57.4",
    "ai": "^5.0.47",
    "bullmq": "^5.1.4",
    "date-fns": "^3.3.1",
    "dotenv": "^17.2.2",
    "ioredis": "^5.3.2",
    "mjml": "^4.14.1",
    "next": "14.0.4",
    "pg": "^8.16.3",
    "postmark": "^4.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.10.3",
    "resend": "^3.0.0",
    "twilio": "^4.19.3"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "prettier": "^3.2.4",
    "turbo": "^1.11.3",
    "typescript": "^5.3.3"
  },
  "packageManager": "pnpm@8.14.1",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  }
}
